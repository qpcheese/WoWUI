["custom"] = "function(states, event, id, _, spellID)     \n    if event == \"UNIT_SPELLCAST_SUCCEEDED\" and spellID == 389843 then        \n        aura_env.queueUpdate()\n        \n        return true \n    end\n    \n    local isEmpty = true\n    \n    for k in pairs(states) do \n        isEmpty = false\n        break\n    end\n    \n    if ((event == \"STATUS\" or event == \"OPTIONS\") and isEmpty) or (event == \"WHELP_TRAINING\" and id == aura_env.id) then \n        aura_env.nextFrame = nil\n        \n        local hasChanges = false\n        \n        for i = 2148, 2153 do\n            local currency = C_CurrencyInfo.GetCurrencyInfo(i)\n            \n            local bracketIndex = string.find(currency.name, \"%(\")\n            local name = currency.name\n            \n            if bracketIndex then\n                name = string.sub(name, bracketIndex + 1, #name - 1)\n            end\n            \n            name = name .. \" (\" .. currency.quantity .. \"/6)\"\n            \n            if states[i] then            \n                if states[i].value ~= currency.quantity then\n                    hasChanges = true\n                    \n                    states[i].changed = true\n                    states[i].value = currency.quantity        \n                    states[i].name = name        \n                end           \n            else\n                hasChanges = true\n                \n                states[i] = {\n                    show = true,\n                    changed = true,\n                    name = name,\n                    progressType = \"static\",\n                    value = currency.quantity,\n                    total = 6,\n                    index = i,\n                }  \n            end\n        end\n        \n        return hasChanges    \n    end\n    \n    return false\nend",
